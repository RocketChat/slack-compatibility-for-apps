    "url": "https://github.com/RocketChat/slack-compatibility-for-apps/issues"
  },
  "homepage": "https://github.com/RocketChat/slack-compatibility-for-apps#readme",
  "dependencies": {
    "@rocket.chat/apps-engine": "^1.12.0",
    "fs-extra": "^9.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/faker": "^4.1.12",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "faker": "^4.1.0",
    "mocha": "^7.2.0",
